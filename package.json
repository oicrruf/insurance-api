{
  "name": "insurance-api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "nodemon ./bin/www",
    "test": "jest --verbose --detectOpenHandles",
    "migrations:generate": "sequelize-cli migration:generate --name",
